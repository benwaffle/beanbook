<section>
  {{#each beans}} {{#with this}}
  <div class="card">
    <div class="card-image">{{imageUrl}}</div>
    <div class="card-content">
      <div class="media">
        <div class="media-content">
          <p class="title">
            <a href="/bean/{{_id}}">{{title}}</a>
          </p>
          <p class="subtitle">@{{creatorId}}</p>
        </div>
      </div>
      <div class="content">
        {{description}}
        <br>
        <time>{{moment timestamp}}</time>
      </div>
    </div>
  </div>
  {{/with}} {{/each}}
</section>